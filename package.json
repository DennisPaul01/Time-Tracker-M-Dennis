{
  "name": "time-tracker",
  "version": "1.0.0",
  "description": "![Design preview for the Time tracking dashboard coding challenge](./design/desktop-preview.jpg)",
  "main": "index.js",
  "scripts": {
    "node:sass": "node-sass scss/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel node:sass devserver",
    "scss-compile": "node-sass scss/main.scss css/style.comp.css",
    "concat:css": "css-concat css/style.comp.css css/style.concat.css",
    "compress-css": "cleancss -o css/style.css css/style.concat.css",
    "build": "npm-run-all scss-compile concat:css compress-css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^6.0.1"
  },
  "dependencies": {
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5"
  }
}
